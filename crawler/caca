    $(document).ready(function() {
        $(".go-to-top").click(function () {
            $(window).scrollTop(0);
        });

        $("#btn-search").click(function(){
            $("#search_front").toggle();
            $("#nav-header").toggle();
            $("#logo").toggle();
            $("#mobile-menu").toggle();
            if ($("#main-nav").hasClass("w-100")) $("#main-nav").removeClass("w-100");
            else $("#main-nav").addClass("w-100");
            $("#s").focus();
        });

        $("#close-search,#close-search-2").click(function(){
            $("#search_front").toggle();
            $("#nav-header").toggle();
            $("#logo").toggle();
            $("#mobile-menu").toggle();
            if ($("#main-nav").hasClass("w-100")) $("#main-nav").removeClass("w-100");
            else $("#main-nav").addClass("w-100");
        });

        $(".resize").click(function(){
            $('#form-size').submit();
        });
        $(".resize-foto").click(function(){
            $('#form-size-foto').submit();
        });
    });

