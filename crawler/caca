            var quill = new Quill('#editor-container', {
                modules: {
                    toolbar: '#toolbar-container'
                },
                placeholder: 'Write a comment...',
                theme: 'snow'
            });

            $('.formcomentaris').submit(function() {
                $("input[name='comentari']").val(quill.root.innerHTML);
            });

            $("#form-comentaris-id").submit(function(e) {
                e.preventDefault();

                let form = $(this);
                let url = form.attr('action');

                $.ajax({
                    type: "POST",
                    url: url,
                    data: form.serialize(),
                    beforeSend: function() {
                        $('#loading').modal('show');
                    },
                    success: function(data)
                    {
                        if (data == 1) {
                            putGeneralMessage("Comment sent successfully.","success");
                            let date = new Date();
                            let sdate = date.toJSON().slice(0,10) + " " + date.getHours() + ":" + date.getMinutes() + ":" + date.getSeconds();
                            addComment($("input[name=comentari]").val(),0,$("input[name=username]").val(),sdate,true,999999999);
                        } else if (data == 0) {
                            putGeneralMessage("An error has occured.","error");
                        } else if (data == -1) {
                            putGeneralMessage("The verification code wasn't entered correctly. Go back and try it again..","error");
                        }
                        $('#loading').modal('hide');
                        quill.setText('');
                    }
                });
            });

            function deleteComment(id,e) {
                if (!e) e = window.event;
                var sender = e.srcElement || e.target;
                var parametres = {
                    "id" : id
                };
                $.ajax({
                    data:  parametres,
                    url:   '../../php/delete_comment.php',
                    type:  'post',
                    beforeSend: function() {
                        //$('#loading').modal('show');
                    },
                    success:  function (resp) {
                        var response = resp.trim();
                        if (response == "1") {
                            putGeneralMessage("Comment deleted successfully.","success");
                            $(sender).closest(".comentari").remove();
                        } else {
                            putGeneralMessage("An error has occured.","success");
                        }
                        //$('#loading').modal('hide');
                    }
                });
            }

            function addComment(com,marg,user,data,meu,idc) {
                var comment = "<div class='comentari' style='margin-left:"+marg+"px;background-color:#F9F9F9'><div class='row align-items-center'>";
                comment += "<div class='col-auto text-left'><p class='font-weight-bold f15 pl-2 pr-2 mb-1 pt-2'>"+user+"</p><p class='f14 pl-2 pr-2  mb-1'>"+data+"</p></div>";
                comment += "<div class='col f16'>" + com + "</div>";
                if (meu) comment += "<div class='col-auto'><a href='javascript:void(0)' onclick='deleteComment(" + idc + ",event);' class='text-gris f12 pl-2 pr-2' title='Delete'><i class='far fa-trash-alt'></i></a></div>";
                else comment += "<div class='col-auto'><a href='javascript:void(0)' onclick='related(" + idc + ");' class='text-gris f12 pl-2 pr-2' title='Reply'><i class='fas fa-reply'></i></a></div>";
                comment += "</div></div>";
                $(".divcomentaris").append(comment);
            }

            function showNoVistos(novistos) {
                var item = "<div class='pt-2 pb-2 alert alert-warning'>There are "+novistos+" additional comments only visible for registered users</div>";
                $(".divcomentaris").append(item);
            }

                    
